{
    "semi": false,
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "traillingComma": "all",
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "overrides": [{ "files": "*.html", "options": { "parser": "babel" } }]
}
